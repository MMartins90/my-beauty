<%= simple_form_for (@beautician) do |f| %>
  <p><%= f.input :name %></p>
  <p><%= f.input :location %></p>
  <p><%= f.input :description %></p>
  <p><%= f.input :Banner_photo, as: :file %></p>
  <p><%= f.input :Portfolio_photos, as: :file, input_html: { multiple: true } %></p>
  <p> <%= f.button :submit, class: "btn btn-primary" %></p
<% end %>